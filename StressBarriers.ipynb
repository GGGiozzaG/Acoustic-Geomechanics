Dataset2.Sigh_ani_norm=Dataset2.Sigh_ani./Dataset2.SigV_psi;
Dataset2.SigH_ani_norm=Dataset2.SigH_ani./Dataset2.SigV_psi;
Dataset2.Sigh_ane_norm=Dataset2.Sigh_ane./Dataset2.SigV_psi;
Dataset2.SigH_ane_norm=Dataset2.SigH_ane./Dataset2.SigV_psi;

Model.Case1_Sh_norm=Model.Case1_Sh/Model.SV;
